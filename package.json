{
  "name": "calorie-tracker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev:server": "tsc -p tsconfig.server.json -w & node dist-server/api.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build:server": "tsc -p tsconfig.server.json",
    "build:railway": "npm run build && npm run build:server",
    "check": "npm run lint && npm run type-check && npm run build:server",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "start": "node dist-server/api.js",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest"
  },
  "dependencies": {
    "@railway/cli": "^3.0.0",
    "@sentry/node": "^7.0.0",
    "@sentry/react": "^7.0.0",
    "@sentry/tracing": "^7.0.0",
    "i18next": "^23.0.0",
    "react-i18next": "^13.0.0",
    "openai": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.0.0",
    "framer-motion": "^10.0.0",
    "tailwindcss": "^3.0.0",
    "autoprefixer": "^10.0.0",
    "postcss": "^8.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-react-refresh": "^0.3.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
